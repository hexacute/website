{
  "name": "putaindecode.io",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world && rimraf build",
    "format": "yarn format:most && yarn format:re",
    "format:most": "prettier --write \"**/*.{js,json,css,md}\"",
    "format:re": "find ./src -iname '*.re' | xargs -n 1 bsrefmt --in-place && find ./src -iname '*.rei' | xargs -n 1 bsrefmt -i true --in-place",
    "pages-server": "pages start src/App.mjs 3000",
    "test": "bsb -make-world",
    "prerender": "pages build src/App.mjs"
  },
  "authors": [
    "bloodyowl <bloodyowl@icloud.com>",
    "MoOx <m@moox.io>",
    "jojmaht <jojmaht@gmail.com>",
    "zoontek <zoontek@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@rescript/react": "^0.10.1",
    "bs-css-emotion": "^2.2.0",
    "bs-webapi": "^0.13.4",
    "md5": "^2.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rescript-asyncdata": "^2.0.0",
    "rescript-pages": "^2.0.1"
  },
  "devDependencies": {
    "bs-platform": "^9.0.0",
    "prettier": "^1.18.2"
  },
  "prettier": {
    "trailingComma": "all",
    "proseWrap": "always"
  }
}
